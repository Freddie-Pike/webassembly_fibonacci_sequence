  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "asbuild:untouched": "asc assembly/index.ts -b www/build/untouched.wasm -t www/build/untouched.wat --sourceMap --validate --debug",
